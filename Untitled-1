    $scope.createNewFile=function() {
        var request="http://dev-back1.techgrind.asia/scripts/rest.pike?request=/home/akoumi";
        var fd = new FormData();
        var files= document.getElementById('file').files;
        var logindata = JSON.parse(localStorageService.get('logindata'));

        fd.append('file',files[0]);
        $http.post(request, fd, {
            transformRequest: angular.identity,
            headers:{"Authorization":"Basic YWtvdW1pOjEyMzQ1Ng=="}
        })
            .success(function(response){
    	       if(response.response.error.message=="None"){
        	   $route.reload();
}
    	 else{
    		 alert(response.response.error.message);}

    	 
    	 })
     .error(function(){
         alert("Error while uploading");

     });
}


